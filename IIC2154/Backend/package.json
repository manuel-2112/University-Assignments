{
  "name": "backend-grupo-5-capstone",
  "version": "1.0.1",
  "description": "This is the API for the backend of the capstone project.",
  "main": "src/server.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "server:prod": "ts-node-dev --poll --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "server:dev": "ts-node-dev --poll --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "lint": "eslint './src/**/*.ts' './tests/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' './tests/**/*.ts' --fix",
    "prettier": "prettier --check './src/**/*.ts' './tests/**/*.ts'",
    "prettier:fix": "prettier --write './src/**/*.ts' './tests/**/*.ts'",
    "loglevel:change": "runtime-node-refresh",
    "test": "NODE_ENV=development jest",
    "test:windows": "SET NODE_ENV=development& jest",
    "precommit": "npm run lint && npm run prettier && npm run test"
  },
  "author": "Grupo 5 - Capstone Project",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.449.0",
    "@aws-sdk/credential-provider-ini": "^3.450.0",
    "@aws-sdk/credential-provider-node": "^3.450.0",
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "apicache": "^1.6.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-jwt": "^8.4.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.0",
    "husky": "^4.3.0",
    "joi": "^17.5.0",
    "jwks-rsa": "^3.0.1",
    "lusca": "^1.7.0",
    "md5": "^2.3.0",
    "mongoose": "^7.6.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.2",
    "node-mocks-http": "^1.11.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.42.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "runtime-node-refresh": "^1.0.6",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^4.3.0",
    "testcontainers": "^10.10.0",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.15",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.10",
    "@types/node": "^16.11.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^5.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
